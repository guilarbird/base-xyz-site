{
  "buildCommand": "cd client && pnpm install && pnpm build",
  "outputDirectory": "dist/public",
  "framework": "vite",
  "installCommand": "pnpm install",
  "devCommand": "cd client && pnpm dev",
  "env": {
    "VITE_APP_TITLE": "Base XYZ",
    "VITE_APP_LOGO": "/logo.svg"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
